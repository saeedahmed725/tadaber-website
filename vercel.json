{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "index_en.html",
      "use": "@vercel/static"
    },
      {
      "href": "/css/styles.css",
      "rel": "stylesheet"
    },
    {
      "href": "/js/scripts.js",
      "rel": "script"
    },
   
    {
      "src": "/assets/",
      "alt": "Tadaber Logo"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
